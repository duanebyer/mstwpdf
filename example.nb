Cell[BoxData[
 RowBox[{
  RowBox[{"format", "[", "number_Real", "]"}], ":=", 
  RowBox[{"ToString", "[", 
   RowBox[{"ScientificForm", "[", 
    RowBox[{"number", ",", "10", ",", 
     RowBox[{"NumberFormat", "->", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"ToString", "[", 
          RowBox[{"PaddedForm", "[", 
           RowBox[{
            RowBox[{"ToExpression", "[", "#1", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"6", ",", "4"}], "}"}]}], "]"}], "]"}], "<>", 
         "\"\<E\>\"", "<>", 
         RowBox[{"ToString", "[", 
          RowBox[{"PaddedForm", "[", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Head", "[", 
                RowBox[{"ToExpression", "[", "#3", "]"}], "]"}], "==", 
               "Integer"}], ",", 
              RowBox[{"ToExpression", "[", "#3", "]"}], ",", "0", ",", "0"}], 
             "]"}], ",", "2", ",", 
            RowBox[{"NumberPadding", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"\"\<0\>\"", ",", "\"\<\>\""}], "}"}]}], ",", 
            RowBox[{"SignPadding", "\[Rule]", "True"}], ",", 
            RowBox[{"NumberSigns", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"\"\<-\>\"", ",", "\"\<+\>\""}], "}"}]}]}], "]"}], 
