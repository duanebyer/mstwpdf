Cell["\<\
Define a function to write the PDFs as a function of x to a file at a fixed \
value of q.
Extrapolation will be used for x < 10^-6.\:ffff\
\>", "Text",
 CellChangeTimes->{{3.4089638210060453`*^9, 3.408963856210204*^9}, {
  3.408965697527019*^9, 3.408965705350567*^9}, {3.408985274892331*^9, 
  3.408985278727716*^9}, {3.408985317840219*^9, 3.4089853232309237`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"WritePDFvsX", "[", 
   RowBox[{"q_Real", ",", "f_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"flavours", ",", "ix", ",", 
      RowBox[{"nx", "=", "100"}], ",", 
      RowBox[{"xmin", "=", "1.*^-7"}], ",", 
      RowBox[{"xmax", "=", "0.99"}], ",", "x", ",", "line"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"flavours", "=", 
      RowBox[{"{", 
       RowBox[{
       "\"\<bbar\>\"", ",", "\"\<cbar\>\"", ",", "\"\<sbar\>\"", ",", 
        "\"\<ubar\>\"", ",", "\"\<dbar\>\"", ",", "\"\<glu\>\"", ",", " ", 
        "\"\<dn\>\"", ",", "\"\<up\>\"", ",", "\"\<str\>\"", ",", 
        "\"\<chm\>\"", ",", "\"\<bot\>\""}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"filename", "=", 
      RowBox[{"\"\<x\>\"", "<>", 
       RowBox[{"flavours", "[", 
        RowBox[{"[", 
         RowBox[{"f", "+", "6"}], "]"}], "]"}], "<>", 
       "\"\<_vs_x_math.dat\>\""}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"file", "=", 
      RowBox[{"OpenWrite", "[", "filename", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{"\"\< # q = \>\"", "<>", 
        RowBox[{"format", "[", "q", "]"}], "<>", "\"\<\\n\>\""}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{"\"\< #\>\"", "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<x\>\"", ",", "10"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{
          RowBox[{"\"\<x\>\"", "<>", 
           RowBox[{"flavours", "[", 
            RowBox[{"[", 
             RowBox[{"f", "+", "6"}], "]"}], "]"}]}], ",", "12"}], "]"}], "<>",
         
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error+\>\"", ",", "12"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error-\>\"", ",", "12"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error\>\"", ",", "12"}], "]"}], "<>", "\"\<\\n\>\""}]}],
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "=", 
          RowBox[{"10.", "^", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Log", "[", 
              RowBox[{"10", ",", "xmin"}], "]"}], "+", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"ix", "-", "1."}], ")"}], "/", 
               RowBox[{"(", 
                RowBox[{"nx", "-", "1."}], ")"}]}], "*", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"Log", "[", 
                 RowBox[{"10", ",", "xmax"}], "]"}], "-", 
                RowBox[{"Log", "[", 
                 RowBox[{"10", ",", "xmin"}], "]"}]}], ")"}]}]}], ")"}]}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"line", "=", 
          RowBox[{
           RowBox[{"format", "[", "x", "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"xf", "[", 
             RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           
           RowBox[{"format", "[", 
            RowBox[{"delp", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"delm", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"del", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           "\"\<\\n\>\""}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"WriteString", "[", 
          RowBox[{"file", ",", "line"}], "]"}], ";"}], "\[IndentingNewLine]", 
        "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"ix", ",", "1", ",", "nx"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Close", "[", "file", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"\"\<x\>\"", "<>", 
        RowBox[{"flavours", "[", 
         RowBox[{"[", 
          RowBox[{"f", "+", "6"}], "]"}], "]"}]}], ",", 
       "\"\< vs. x for q2 = \>\"", ",", 
       RowBox[{"q", "^", "2"}], ",", "\"\< written to \>\"", ",", 
       "filename"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

