Cell["\<\
Define a function to write the PDFs as a function of q^2 to a file at a fixed \
value of x.
Extrapolation will be used for q^2 < 1 GeV^2 and for q^2 > 10^9 GeV^2.\:ffff\
\>", "Text",
 CellChangeTimes->{{3.408963949120545*^9, 3.408963949504689*^9}, {
  3.408965677504883*^9, 3.408965685271555*^9}, {3.4089657249018908`*^9, 
  3.408965728149137*^9}, {3.4089853361365843`*^9, 3.4089853375033913`*^9}, {
  3.426962454191409*^9, 3.4269624636225977`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"WritePDFvsQ2", "[", 
   RowBox[{"x_Real", ",", "f_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"flavours", ",", "iq", ",", 
      RowBox[{"nq", "=", "100"}], ",", 
      RowBox[{"q2min", "=", "0.5"}], ",", 
      RowBox[{"q2max", "=", "1.*^10"}], ",", "q", ",", "line"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"flavours", "=", 
      RowBox[{"{", 
       RowBox[{
       "\"\<bbar\>\"", ",", "\"\<cbar\>\"", ",", "\"\<sbar\>\"", ",", 
        "\"\<ubar\>\"", ",", "\"\<dbar\>\"", ",", "\"\<glu\>\"", ",", " ", 
        "\"\<dn\>\"", ",", "\"\<up\>\"", ",", "\"\<str\>\"", ",", 
        "\"\<chm\>\"", ",", "\"\<bot\>\""}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"filename", "=", 
      RowBox[{"\"\<x\>\"", "<>", 
       RowBox[{"flavours", "[", 
        RowBox[{"[", 
         RowBox[{"f", "+", "6"}], "]"}], "]"}], "<>", 
       "\"\<_vs_q2_math.dat\>\""}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"file", "=", 
      RowBox[{"OpenWrite", "[", "filename", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{"\"\< # x = \>\"", "<>", 
        RowBox[{"format", "[", "x", "]"}], "<>", "\"\<\\n\>\""}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{"\"\< #\>\"", "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<q2\>\"", ",", "10"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{
          RowBox[{"\"\<x\>\"", "<>", 
           RowBox[{"flavours", "[", 
            RowBox[{"[", 
             RowBox[{"f", "+", "6"}], "]"}], "]"}]}], ",", "12"}], "]"}], "<>",
         
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error+\>\"", ",", "12"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error-\>\"", ",", "12"}], "]"}], "<>", 
        RowBox[{"PadString", "[", 
         RowBox[{"\"\<error\>\"", ",", "12"}], "]"}], "<>", "\"\<\\n\>\""}]}],
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"q", "=", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"10.", "^", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Log", "[", 
               RowBox[{"10", ",", "q2min"}], "]"}], "+", 
              RowBox[{
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"iq", "-", "1."}], ")"}], "/", 
                RowBox[{"(", 
                 RowBox[{"nq", "-", "1."}], ")"}]}], "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"Log", "[", 
                  RowBox[{"10", ",", "q2max"}], "]"}], "-", 
                 RowBox[{"Log", "[", 
                  RowBox[{"10", ",", "q2min"}], "]"}]}], ")"}]}]}], ")"}]}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"line", "=", 
          RowBox[{
           RowBox[{"format", "[", 
            RowBox[{"q", "^", "2"}], "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"xf", "[", 
             RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           
           RowBox[{"format", "[", 
            RowBox[{"delp", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"delm", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           RowBox[{"format", "[", 
            RowBox[{"del", "[", 
             RowBox[{"x", ",", "q", ",", "f"}], "]"}], "]"}], "<>", 
           "\"\<\\n\>\""}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"WriteString", "[", 
          RowBox[{"file", ",", "line"}], "]"}], ";"}], "\[IndentingNewLine]", 
        "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"iq", ",", "1", ",", "nq"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Close", "[", "file", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"\"\<x\>\"", "<>", 
        RowBox[{"flavours", "[", 
         RowBox[{"[", 
          RowBox[{"f", "+", "6"}], "]"}], "]"}]}], ",", 
       "\"\< vs. q2 for x = \>\"", ",", "x", ",", "\"\< written to \>\"", ",",
        "filename"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

