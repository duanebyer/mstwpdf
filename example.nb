Cell["Define a function to pad a string to a certain length.", "Text",
 CellChangeTimes->{{3.408963756880597*^9, 3.408963773893641*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"PadString", "[", 
   RowBox[{"str_String", ",", "len_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"temp", "=", "str"}], "}"}], ",", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"StringLength", "[", "temp", "]"}], "<", "len"}], ",", 
       RowBox[{"temp", "=", 
        RowBox[{"\"\< \>\"", "<>", "temp"}]}]}], "]"}], ";", " ", 
     RowBox[{"Return", "[", "temp", "]"}], ";"}]}], "]"}]}]], "Input",
