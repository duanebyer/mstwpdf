Cell["\<\
Define functions to give the uncertainty on the parton distributions using \
both the formula for asymmetric errors [eqs.(51,52) of arXiv:0901.0002] \
\:ffffand the formula for symmetric errors [eq.(50) of same paper].\:ffff\
\>", "Text",
 CellChangeTimes->{{3.408962488203682*^9, 3.4089625016857*^9}, {
  3.408963569232234*^9, 3.4089636381337337`*^9}, {3.440105112798512*^9, 
  3.440105140966363*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"delp", "[", 
   RowBox[{"x_Real", ",", "q_Real", ",", "f_Integer"}], "]"}], ":=", 
  RowBox[{"Sqrt", "[", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Max", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"2", "i"}], "-", "1"}], ",", "x", ",", "q", ",", "f"}], 
          "]"}], "-", 
         RowBox[{"xf", "[", 
          RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{"2", "i"}], ",", "x", ",", "q", ",", "f"}], "]"}], "-", 
         RowBox[{"xf", "[", 
          RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}]}], ",", "0."}], 
       "]"}], "^", "2"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"nhess", "/", "2"}]}], "}"}]}], "]"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"delm", "[", 
   RowBox[{"x_Real", ",", "q_Real", ",", "f_Integer"}], "]"}], ":=", 
  RowBox[{"Sqrt", "[", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Max", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"xf", "[", 
          RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}], "-", 
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"2", "i"}], "-", "1"}], ",", "x", ",", "q", ",", "f"}], 
          "]"}]}], ",", 
        RowBox[{
         RowBox[{"xf", "[", 
          RowBox[{"0", ",", "x", ",", "q", ",", "f"}], "]"}], "-", 
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{"2", "i"}], ",", "x", ",", "q", ",", "f"}], "]"}]}], ",", 
        "0."}], "]"}], "^", "2"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"nhess", "/", "2"}]}], "}"}]}], "]"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"del", "[", 
   RowBox[{"x_Real", ",", "q_Real", ",", "f_Integer"}], "]"}], ":=", 
  RowBox[{"0.5", "*", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"2", "i"}], "-", "1"}], ",", "x", ",", "q", ",", "f"}], 
          "]"}], "-", 
         RowBox[{"xf", "[", 
          RowBox[{
           RowBox[{"2", "i"}], ",", "x", ",", "q", ",", "f"}], "]"}]}], ")"}],
        "^", "2"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", 
        RowBox[{"nhess", "/", "2"}]}], "}"}]}], "]"}], "]"}]}]}]}], "Input",
